{
  "name": "auto-fill-form",
  "version": "1.0.0",
  "description": "Auto-fill form using Playwright and CSV",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --out-path dist"
  },
  "author": "Pham Huu Nhan",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "playwright": "^1.54.2"
  },
  "pkg": {
    "assets": [
      "data/*.csv",
      "assets/*"
    ],
    "scripts": [
      "config.js"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist",
    "win": {
      "icon": "icon.ico"
    }
  }
}
